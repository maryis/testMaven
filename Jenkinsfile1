pipeline {
    agent any 
    stages {

     stage('compile') {

        steps {
                sh 'mvn compile' 
            }
        }

     stage('test') {
            steps {
                sh 'mvn test' 
            }
        }

     stage('code_review') {
            steps {
                sh 'mvn pmd:pmd' 
            }
        }
   }
}
